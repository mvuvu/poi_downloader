{
  "crawler": {
    "max_workers": 2,
    "driver_pool_size": 2,
    "batch_size": 20,
    "timeout": 15,
    "retry_times": 3,
    "headless": false,
    "checkpoint_interval": 50
  },
  "output": {
    "format": "csv",
    "encoding": "utf-8",
    "include_timestamp": true
  },
  "logging": {
    "level": "INFO",
    "file": "logs/crawler.log",
    "max_size_mb": 10
  }
}